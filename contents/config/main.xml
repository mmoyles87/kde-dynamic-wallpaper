      <default>/home/matt/projects/kde6-dynamic-wallpaper/images/3.jpg</default>
    </entry>
    <entry name="NightImage" type="String">
      <default>/home/matt/projects/kde6-dynamic-wallpaper/images/4.jpg</default>
    </entry>
    <entry name="FillMode" type="Int">
